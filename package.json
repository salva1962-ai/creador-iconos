{
  "name": "creador-iconos",
  "version": "1.0.0",
  "description": "",
  "main": "service-worker.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "npx babel src/scripts/app.jsx --out-file src/scripts/app.js",
    "build:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --minify",
    "dev": "concurrently \"npm:dev:js\" \"npm:dev:css\" \"npx serve -l 3000 .\"",
    "dev:js": "npx babel src/scripts/app.jsx --out-file src/scripts/app.js --watch",
    "dev:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/output.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salva1962-ai/creador-iconos.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/salva1962-ai/creador-iconos/issues"
  },
  "homepage": "https://github.com/salva1962-ai/creador-iconos#readme",
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "autoprefixer": "^10.4.24",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19"
  }
}
